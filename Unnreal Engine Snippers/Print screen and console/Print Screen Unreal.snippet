	<!--<FVector>-->
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Print Screen FVector Unreal </Title>
			<Author>Ariel Gimenez</Author>
			<Description>To print a message in screen of type FVector in unreal engine</Description>
			<!--<Shortcut> Alt </Shortcut>-->
		</Header>
		<Snippet>
			<Code Language="CPP">
			<![CDATA[ 	const FVector position_to_print = FVector::ZeroVector;
						if (GEngine) GEngine->AddOnScreenDebugMessage(-10, 3.0f, FColor::Yellow, FString::Printf(TEXT("Position: %f - %f - %f"), position_to_print.X, position_to_print.Y, position_to_print.Z));]]>
			</Code>
		</Snippet>
	</CodeSnippet>


	<!--<FRotator>-->
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Print Screen FRotator Unreal </Title>
			<Author>Ariel Gimenez</Author>
			<Description>To print a message in screen of type FRotator in unreal engine</Description>
			<!--<Shortcut> Alt </Shortcut>-->
		</Header>
		<Snippet>
			<Code Language="CPP">
				<![CDATA[ 	const FRotator rotation_to_print = GetActorRotation();
							if (GEngine) GEngine->AddOnScreenDebugMessage(-10, 3.0f, FColor::Yellow, FString::Printf(TEXT("Rotation: %f - %f - %f"), rotation_to_print.Pitch, rotation_to_print.Yaw, rotation_to_print.Roll));]]>
			</Code>
		</Snippet>
	</CodeSnippet>

	<!--<Pring FName>-->
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Print Screen FName Unreal </Title>
			<Author>Ariel Gimenez</Author>
			<Description>to print messages in console concatenated of types int,float,FString  </Description>
			<!--<Shortcut> Alt </Shortcut>-->
		</Header>
		<Snippet>
			<Code Language="CPP">
				<![CDATA[ 	FName test_FName = TEXT("Test Fname");
							FString new_text_print_concatenate = "hello: " + test_FName.ToString();
							if (GEngine) GEngine->AddOnScreenDebugMessage(-1, 3.0f, FColor::Yellow, *new_text_print_concatenate);]]>
			</Code>
		</Snippet>
	</CodeSnippet>

	<!--<Pring Bool>-->
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Print Screen Bool Unreal </Title>
			<Author>Ariel Gimenez</Author>
			<Description>To print a message in screen of type Bool in unreal engin  </Description>
			<!--<Shortcut> Alt </Shortcut>-->
		</Header>
		<Snippet>
			<Code Language="CPP">
				<![CDATA[ 	bool my_test_bool = false;
