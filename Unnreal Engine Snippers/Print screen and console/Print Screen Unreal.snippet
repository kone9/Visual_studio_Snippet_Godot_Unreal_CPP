	<!--<FString>-->
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Print a Screen mensaje Unreal </Title>
			<Author>Ariel Gimenez</Author>
			<Description>To print a message in screen of type FString in unreal engine</Description>
			<!--<Shortcut> Alt </Shortcut>-->
		</Header>
		<Snippet>
			<Code Language="CPP">
				<![CDATA[ if(GEngine) GEngine->AddOnScreenDebugMessage(-1, 3.0f, FColor::Yellow, TEXT("Some debug message!"));]]>
			</Code>
		</Snippet>
	</CodeSnippet>

	<!--<Float>-->
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Print Screen Float Unreal </Title>
			<Author>Ariel Gimenez</Author>
			<Description>To print a message in screen of type Float in unreal engine</Description>
			<!--<Shortcut> Alt </Shortcut>-->
		</Header>
		<Snippet>
			<Code Language="CPP">
				<![CDATA[ const float float_to_print = 1.0f;
						  if (GEngine) GEngine->AddOnScreenDebugMessage(-1, 15.0f, FColor::Yellow, FString::Printf(TEXT("float: %f"), float_to_print));]]>
			</Code>
		</Snippet>
	</CodeSnippet>

